{
  "prompts": {
    "name": {
      "type": "string",
      "required": true,
      "label": {
        "en": "Project name",
        "zh_CN": "项目名称"
      }
    },
    "description": {
      "type": "string",
      "required": true,
      "label":{
        "en":  "Project description",
        "zh_CN": "项目描述"
      },
      "default": "A Vue.js project"
    },
    "author": {
      "type": "string",
      "label": {
        "en":  "Author",
        "zh_CN": "作者"
      }
    },
    "license": {
      "type": "string",
      "label": {
        "en":  "License",
        "zh_CN": "许可证"
      },
      "default": "MIT"
    },
    "sass": {
       "type": "confirm",
       "label": {
        "en":  "Use sass?",
        "zh_CN": "使用 sass?"
      },
       "default": false
    }
  },
  "completeMessage": "{{#inPlace}}\n\n  cd {{destDirName}}\n  npm install\n  npm run dev{{/inPlace}}"
}